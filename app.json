{
  "expo": {
    "name": "FanCave",
    "slug": "FanCave",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "28242acf-dcdf-43d5-9e7e-ee9eea987f78"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/28242acf-dcdf-43d5-9e7e-ee9eea987f78"
    },
    "plugins": [
      [
        "expo-av",
        {
          "microphonePermission": "Allow FanCave to access your microphone."
        }
      ]
    ]
  }
}
